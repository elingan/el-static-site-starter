{
  "name": "el-static-site-starter",
  "version": "0.0.1",
  "description": "Static Site Generator with TailwindCSS",
  "main": "index.js",
  "scripts": {
    "start": "DEBUG=sumaq* node ./tasks/webserver.js",
    "dev": "DEBUG=sumaq* node ./tasks/webserver.js",
    "server": "wds -c ./tasks/_config.js",
    "build": "DEBUG=sumaq*  NODE_ENV=production node -e 'require(\"./tasks/build.js\").buildAll()'",
    "publish": "npm run build && netlify deploy --dir=dist --prod --message \"Manual deploy from npm\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@web/dev-server": "0.0.15",
    "autoprefixer": "^10.0.1",
    "cssnano": "^4.1.10",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "js-yaml": "^3.14.0",
    "node-watch": "^0.7.0",
    "nodemon": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.2",
    "postcss-discard-comments": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^5.0.1",
    "pug": "^3.0.0",
    "rollup": "^2.32.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^1.9.4"
  },
  "dependencies": {}
